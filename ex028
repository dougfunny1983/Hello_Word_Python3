print('''#############################
##Jogo da Adivinhação v.1.0##
#############################''')
from random import randint
print('→←'*30)
com = randint(1,5)
jog = int(input('Digite um numero entre 1 e 5: '))
if com == jog:
    print(f'O computador jogou {com} e voce jogou {jog}')
    print('Parabéns!!! Voce venceu!!!')
else:
    print(f'O computador jogou {com} e voce jogou {jog}')
    print('Infelismente voce perdeu')
print('←→'*30)